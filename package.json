{
  "name": "ketchup-server",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build:server": "babel ./src --out-dir ./build --source-maps",
    "build": "npm run clean && npm run build:server",
    "start": "babel-node src/index.js",
    "start:prod": "knex migrate:latest && node build/index.js",
    "pretest": "env NODE_ENV=${NODE_ENV:-test} knex migrate:latest",
    "test": "env NODE_ENV=${NODE_ENV:-test} ava **/*.spec.js --serial",
    "test:watch": "npm test -- --watch",
    "test:up": "npm test -- --update-snapshots",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ertrzyiks/ketchup-server.git"
  },
  "author": "ertrzyiks <mateusz.derks@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ertrzyiks/ketchup-server/issues"
  },
  "homepage": "https://github.com/ertrzyiks/ketchup-server#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2017": "^6.24.1",
    "body-parser": "^1.17.2",
    "bookshelf": "^0.10.4",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "logux-core": "github:logux/logux-core",
    "logux-server": "github:logux/logux-server",
    "logux-sync": "github:logux/logux-sync",
    "object-path": "^0.11.4",
    "pg": "^7.1.2",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "username-generator": "^1.0.3"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "eslint": "^4.5.0",
    "knex-cleaner": "^1.1.3",
    "sinon": "^3.0.0",
    "sqlite3": "^3.1.8"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
